<?php
/**
 * @var \Laminas\View\Renderer\PhpRenderer $this
 * @var \EasyAdmin\Form\CheckAndFixForm $form
 */

$plugins = $this->getHelperPluginManager();
$escape = $plugins->get('escapeHtml');
$assetUrl = $plugins->get('assetUrl');
$translate = $plugins->get('translate');

$form->prepare();

$this->htmlElement('body')->appendAttribute('class', 'easy-admin check-and-fix browse');

$this->headScript()
    ->appendFile($assetUrl('js/easy-admin.js', 'EasyAdmin'), 'text/javascript', ['defer' => 'defer'])
;
?>

<?= $this->pageTitle($translate('Check and fix'), 1, $translate('Easy Admin')) ?>

<h2><?= $escape($translate('Checks and fixes')) ?></h2>

<?= $this->form()->openTag($form) ?>
<div id="page-actions">
    <button type="submit"><?= $translate('Submit') ?></button>
</div>
<?= $this->formCollection($form) ?>
<?= $this->form()->closeTag() ?>
